# SSH config for work-related servers
Host ics-*
  User yxli
  IdentityFile ~/.ssh/ics-server
  ControlMaster auto
  ControlPath ~/.ssh/sockets/%r@%h-%p
  ControlPersist 1h

Host ics-vm
  HostName igw.dfshan.net
  Port 2291
  IdentityFile ~/.ssh/ics-vm

Host ics-server
  HostName x86.ics.xjtu-ants.net
  Port 2291

Host ics-server-new
  HostName x86-xq.ics.xjtu-ants.net
  Port 2291

Host ics-external
